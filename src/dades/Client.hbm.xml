<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="domini.Client" table="CLIENT">
  <id column="DNI" name="dni" type="java.lang.String"/>
  <property name="nom" type="java.lang.String" column="NOM"/>
  <property name="cognoms" type="java.lang.String" column="COGNOMS"/>
  <property name="email" type="java.lang.String" column="EMAIL"/>
 </class>
</hibernate-mapping>
