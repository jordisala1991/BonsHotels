CREATE MEMORY TABLE PUBLIC.PREUTIPUSHABITACIO(NOMHOTEL VARCHAR(50),NOMTIPUSHABITACIO VARCHAR(50),PREU DOUBLE,PERCENTATGE DOUBLE,DESCOMPTE DOUBLE,PRIMARY KEY(NOMHOTEL,NOMTIPUSHABITACIO),FOREIGN KEY(NOMHOTEL) REFERENCES PUBLIC.HOTEL(NOM),FOREIGN KEY(NOMTIPUSHABITACIO) REFERENCES PUBLIC.TIPUSHABITACIO(NOM),CHECK(((PUBLIC.PREUTIPUSHABITACIO.PERCENTATGE IS NOT NULL) OR (PUBLIC.PREUTIPUSHABITACIO.DESCOMPTE IS NOT NULL)) AND (NOT ((PUBLIC.PREUTIPUSHABITACIO.PERCENTATGE IS NOT NULL) AND (PUBLIC.PREUTIPUSHABITACIO.DESCOMPTE IS NOT NULL)))))
