CREATE MEMORY TABLE PUBLIC.CLIENT(DNI CHARACTER(9) PRIMARY KEY,NOM VARCHAR(50),COGNOMS VARCHAR(50),EMAIL VARCHAR(50))
CREATE MEMORY TABLE PUBLIC.RESERVA(DNICLIENT CHARACTER(9),NOMHOTEL VARCHAR(50),NUMEROHABITACIO INTEGER,DATAINICI DATE,DATARESERVA DATE,DATAFI DATE,IDRESERVA VARCHAR(255),PREUTOTAL DOUBLE,PRIMARY KEY(DNICLIENT,NOMHOTEL,NUMEROHABITACIO,DATAINICI),FOREIGN KEY(DNICLIENT) REFERENCES PUBLIC.CLIENT(DNI),FOREIGN KEY(NOMHOTEL,NUMEROHABITACIO) REFERENCES PUBLIC.HABITACIO(NOMHOTEL,NUMERO))
CREATE MEMORY TABLE PUBLIC.COMENTARI(NOMHOTEL VARCHAR(50),DNICLIENT CHARACTER(9),DATE DATE,AVALUACIO DOUBLE,DESCRIPCIO VARCHAR(255),PRIMARY KEY(NOMHOTEL,DNICLIENT,DATE),FOREIGN KEY(NOMHOTEL) REFERENCES PUBLIC.HOTEL(NOM),FOREIGN KEY(DNICLIENT) REFERENCES PUBLIC.CLIENT(DNI))
CREATE MEMORY TABLE PUBLIC.BONSHOTELS(QUOTAFIXA DOUBLE,QUOTAVARIABLE DOUBLE,PRIMARY KEY(QUOTAFIXA,QUOTAVARIABLE))
